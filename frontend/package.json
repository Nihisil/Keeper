    "typescript": "^4.2.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$VERSION react-scripts start",
    "build": "REACT_APP_VERSION=$VERSION react-scripts build",
    "//": "Linters and tests:",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html tree.html",
    "test": "CI=true react-scripts test",
    "format": "eslint src/**/*.ts{,x} --fix && prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --max-warnings=0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
